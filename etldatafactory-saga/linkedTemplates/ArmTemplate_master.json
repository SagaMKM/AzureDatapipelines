{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "etldatafactory-saga"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"CopyDataETLFromBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CopyDataETLFromBlob'"
		},
		"LinkedServiceETLtoDatalakeGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkedServiceETLtoDatalakeGen2'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eltsqlserverfordatabase.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "master"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sagamkm"
		},
		"LinkedServiceETLtoDatalakeGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://etldatalakegen2.dfs.core.windows.net/"
		},
		"ETL_Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6b0bfc13-10cd-404c-ad16-714ae884c74b/resourceGroups/ETLresourcegroup/providers/Microsoft.Storage/storageAccounts/etlblobstorge"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "etldatafactory-saga_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"CopyDataETLFromBlob_connectionString": {
						"value": "[parameters('CopyDataETLFromBlob_connectionString')]"
					},
					"LinkedServiceETLtoDatalakeGen2_accountKey": {
						"value": "[parameters('LinkedServiceETLtoDatalakeGen2_accountKey')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"LinkedServiceETLtoDatalakeGen2_properties_typeProperties_url": {
						"value": "[parameters('LinkedServiceETLtoDatalakeGen2_properties_typeProperties_url')]"
					},
					"ETL_Trigger_properties_typeProperties_scope": {
						"value": "[parameters('ETL_Trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}